{
  "name": "textile-buckets",
  "version": "0.0.0",
  "private": true,
  "description": "Create, update, destroy remote directories on IPFS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "basic": "npm run build && npm run pack && npm test",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test",
    "textile:check": "npx ncu '/^@textile/.*$/'",
    "textile:upgrade": "npx ncu -u '/^@textile/.*$/'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "textile.io",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@textile/buckets": "^0.5.1",
    "@textile/context": "^0.6.4",
    "glob": "^7.1.6",
    "isomorphic-ws": "^4.0.1",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^3.1.0",
    "@zeit/ncc": "^0.22.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-plugin-github": "^4.0.1",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "js-yaml": "^3.14.0",
    "npm-check-updates": "^8.1.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.6.4"
  }
}
